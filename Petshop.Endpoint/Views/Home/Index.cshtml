@model List<Product>
<div class="content">
    <ul>
        @foreach (var item in Model)
        {
            
        <li>
            <a href="#">
               @* @item.Image*@
                <img src="~/images/newimgs/51t9WZ3GNiL.jpg" width="114" height="160" alt=""></a>
            <h2>@item.Name</h2>
            <h6>@item.Price.ToString("c")</h6>
            <p>@item.Description<a class="more" href="#">View all</a></p>
        </li>
        }
      
    </ul>
</div>
